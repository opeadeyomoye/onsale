{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "onsale-worker-main",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-14",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "workers_dev": false,
