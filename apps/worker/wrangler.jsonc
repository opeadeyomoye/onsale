  "r2_buckets": [
    {
      "binding": "PRODUCT_MEDIA",
      "bucket_name": "onsale-product-media"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "onsale-db",
      "database_id": "b86ff3d4-691d-44cd-ad80-71fa3820530b",
      "migrations_dir": "drizzle"
    }
  ],
